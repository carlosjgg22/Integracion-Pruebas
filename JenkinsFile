pipeline {
	agent any
	tools{
	maven 'Maven 3.6.3'
	jdk 'jdk8'
	
	} 

	stages{
		stage('Compile Stage'){
		    steps{
		     bat 'mvn compile'		
		        }		
		    }	

       stage('Test Stage'){
		    steps{
		     bat 'mvn test'		
		    	
		    }
	    }	

       stage('Deploy Stage'){
		    steps{
		     bat 'mvn deploy'		
		        }		
		    }
	    }	
}